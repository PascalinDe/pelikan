{
  "name": "pelikan",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.24",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/sqlite3": "^3.1.8",
    "express": "^4.18.2",
    "onchange": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.6",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:react-app": "react-scripts start",
    "start:express-server": "node express-server/src/app.js",
    "build:react-app": "react-scripts build",
    "build:express-server": "npx tsc -p express-server/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:react-app": "onchange 'src/**/*.ts' 'src/**/*.tsx' -- npm run build:react-app",
    "watch:express-server-ts": "onchange 'express-server/src/**/*.ts' -- npm run build:express-server",
    "watch:express-server-js": "onchange -i -k 'express-server/src/**/*.js' -- node express-server/src/app.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
